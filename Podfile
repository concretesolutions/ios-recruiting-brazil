platform :ios, '13.0'

source 'https://github.com/CocoaPods/Specs.git'

target 'theMovie-app' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for theMovie-app
  def testing_pods
    pod 'Quick'
    pod 'Nimble'
  end

  target 'theMovie-appTests' do
    testing_pods
  end

  pod 'Kingfisher', '~> 5.0'

end
