# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'DesafioConcrete' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DesafioConcrete
  pod 'SDWebImage', '~> 4.0'
  pod "TMDBSwift"

  #Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'


	

  target 'DesafioConcreteTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DesafioConcreteUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
